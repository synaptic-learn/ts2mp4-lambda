{
  "name": "ts2mp4-lambda",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "sh scripts/build.sh",
    "deploy": "yarn build && yarn zip && yarn upload",
    "dev": "nodemon",
    "start": "node dist/index.js",
